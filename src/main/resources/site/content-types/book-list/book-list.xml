<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <display-name>Book list</display-name>
  <description>Select directly from Google Books API</description>
  <super-type>base:structured</super-type>
  <form>
    <input name="books" type="CustomSelector">
      <label>Books</label>
      <occurrences minimum="0" maximum="0"/>
      <config>
        <service>book-selector</service>
      </config>
    </input>
  </form>
</content-type>
